<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:controller="user_MemberList.FX_Controller" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefWidth="200.0">
         <VBox.margin>
            <Insets top="18.0" />
         </VBox.margin>
         <children>
            <Label text="회원 ID   :">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="id" prefWidth="300.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label text="회원이름 :">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="name" prefWidth="300.0" />
         </children>
         <VBox.margin>
            <Insets top="3.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <VBox.margin>
            <Insets top="3.0" />
         </VBox.margin>
         <children>
            <Label text="회원전화 :">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tel" prefWidth="300.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Label text="회원주소 :">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="addr" prefWidth="300.0" />
         </children>
         <VBox.margin>
            <Insets top="3.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="200.0">
         <children>
            <Button fx:id="add" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="추 가" onAction="#AddClicked">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="edit" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="수 정" onAction="#EditClicked">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="del" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="삭 제" onAction="#DelClicked">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="ok" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="확 인" >
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="no" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="취 소"  />
         </children>
      </HBox>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="culId" prefWidth="70.0" text="회원ID" />
          <TableColumn fx:id="culName" prefWidth="200.0" text="회원이름" />
            <TableColumn fx:id="culTel" prefWidth="165.0" text="회원전화" />
            <TableColumn fx:id="culAddr" prefWidth="165.0" text="회원주소" />
        </columns>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </TableView>
   </children>
</VBox>
